        ["Spread"] = {
            ["Move"] = 0,
            ["Crouch"] = 0,
            ["Stand"] = 0,
            ["MaxInaccuracy"] = 0,
            ["Land"] = 0,
            ["Value"] = 0,
            ["Jump"] = 0,
            ["Ladder"] = 0,
            ["RecoveryTime"] = {
                ["Value"] = 0,
                ["Crouched"] = 0
            }
        },
        ["AccuracyOffset"] = 0,
        ["ArmorPenetration"] = 40007,
        ["ReloadTime"] = 0,
        ["Auto"] = false,
        ["Burst"] = 5,
        ["Ammo"] = ammo,
        ["PointsAward"] = 1000,
        ["RangeModifier"] = 9000,
        ["EquipTime"] = 0,
        ["StoredAmmo"] = storageAmmo,
        ["Secondary"] = true,
        ["Name"] = "Glock",
        ["Tagging"] = 1,
        ["KillAward"] = 1000000,
        ["AccuracyDivisor"] = 0,
        ["Cooldown"] = 0.00000001,
        ["Recoil"] = {
            ["Value"] = 0.0000001,
            ["X"] = 0,
            ["Y"] = 0
        }
    },
    [2] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame,
    [3] = ammo,
    [4] = storageAmmo,
    [5] = false,
    [6] = game:GetService("Players").LocalPlayer,
    [7] = false,
    [8] = false
}

game:GetService("ReplicatedStorage").Events.Drop:FireServer(unpack(args))
end)
end

Test(tonumber(n1),tonumber(n2),tonumber(n3))
return
end)

    end
end
