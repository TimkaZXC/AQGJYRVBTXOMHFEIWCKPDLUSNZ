    if plr.Name == n then
Test(plr)
    end
  end
return
end)

Main:Toggle("[üêä]Spin", Color3.fromRGB(0,255,0), function()
local function Test(s)
pcall(function()
local args = {
    [1] = s,
    [2] = false
}

game:GetService("ReplicatedStorage").Events.Control:FireServer(unpack(args))
local args = {
    [1] = s,
    [2] = true
}

game:GetService("ReplicatedStorage").Events.Control:FireServer(unpack(args))
end)
end

local random = math.random(0, 100)
if random <= 51 then
Test(math.random())
elseif random >= 50 then
Test(-math.random())
end

return
end)


