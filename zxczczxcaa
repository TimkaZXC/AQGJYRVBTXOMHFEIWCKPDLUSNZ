    if weapon then

Weapon:Change("[üêä]"..weapon.Name, 3, 1, Color3.fromRGB(0,255,0), function(n1, n2, n3)
local function Test(ammo, storageAmmo, rate)
pcall(function()
local args = {
    [1] = {
        ["MinDmg"] = 0,
        ["Animations"] = weapon.Configuration,
        ["Bullets"] = 90,
        ["Range"] = 4096,
        ["FireRate"] = rate,
        ["CanDrop"] = true,
        ["DMG"] = 3000,
        ["Penetration"] = 1000,
