local Weapons = Hub:Section("[‚öî]Weapons (Amount)")
Weapons:Title("[‚öî]GETTING", Color3.fromRGB(255,255,255))

for _, item in pairs(game:GetService("ReplicatedStorage").Weapons:GetChildren()) do
if item:IsA("Tool") then
Weapons:Change("[‚öî]"..item.Name.." DMG: "..item.DMG.Value, 1, 4, Color3.fromRGB(255,255,255), function(n)
local function Test(s)
local args = {
    [1] = item.Name,
    [2] = s,
    [3] = false
}

game:GetService("ReplicatedStorage").Events2.CraftingAddingAndRemovingFromInventory:FireServer(unpack(args))
end

Test(tonumber(n))
return
end)
end
end

