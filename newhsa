local function Test(s)
local args = {
    [1] = "Experience",
    [2] = s,
    [3] = 0
}

game:GetService("ReplicatedStorage").Events2.LevelingSystemChanging:FireServer(unpack(args))
end

Test(tonumber(n))
return
end)

