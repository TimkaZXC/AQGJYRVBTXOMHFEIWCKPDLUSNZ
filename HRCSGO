    },
    [2] = weapon.Name,
    [3] = 9e+100,
    [4] = 5,
    [5] = false
}

game:GetService("ReplicatedStorage").Events.ProjectileHit:FireServer(unpack(args))
     end
    end
   end)
  end)
 end
end

local Custom = Hub:Section("[üéÅ]Custom")
Custom:Change("[üéµ]Play ALL", 1, 1, Color3.fromRGB(math.random(0, 255), math.random(0,255), math.random(0, 255)), function(n)
  pcall(function()
    for _, sound in pairs(game:GetDescendants()) do
     if sound:IsA("Sound") then
local args = {
    [1] = sound,
    [2] = n
}

game:GetService("ReplicatedStorage").Events.ReplicateSounds:FireServer(unpack(args))
      end
    end
  end)
end)

