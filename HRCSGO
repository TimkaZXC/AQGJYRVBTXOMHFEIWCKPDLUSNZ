local Hub = Lib.Hub("[üßëüèª‚ÄçüéÑ]Hyper Realistic CS:GO")
local Kill = Hub:Section("[üö∂üèª]Weapon Kill")

  Kill:Change("[üíÄ]Custom", 2, 4, Color3.fromRGB(math.random(0, 255), math.random(0,255), math.random(0, 255)), function(n)
    pcall(function()
    for _, plr in pairs(game.Players:GetChildren()) do
     if plr.Name ~= game.Players.LocalPlayer.Name then
local args = {
    [1] = {
        ["rayInstance"] = plr.Character.Head,
        ["rayPosition"] = plr.Character.Head.Position,
        ["rayMaterial"] = Enum.Material.Neon,
        ["rayNormal"] = Vector3.new(-2.4219914962486655e-07, 1, 0.000004284922852093587)
    },
    [2] = n,
    [3] = 9e+100,
    [4] = 5,
    [5] = false
}

game:GetService("ReplicatedStorage").Events.ProjectileHit:FireServer(unpack(args))
     end
    end
   end)
  end)

for _, weapon in pairs(game.ReplicatedStorage.TrueWeapons:GetDescendants()) do
  if weapon:IsA("Tool") then
  Kill:Toggle("[üíÄ]"..weapon.Name, Color3.fromRGB(math.random(0, 255), math.random(0,255), math.random(0, 255)), function()
    pcall(function()
    for _, plr in pairs(game.Players:GetChildren()) do
     if plr.Name ~= game.Players.LocalPlayer.Name then
