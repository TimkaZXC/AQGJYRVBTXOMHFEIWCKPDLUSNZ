local Item = Hub:Section("[ğŸ²]Items")
Item:Title("[ğŸ²]GETTING", Color3.fromRGB(255,255,255))

for _, item in pairs(game:GetService("ReplicatedStorage").Items:GetChildren()) do
if item:IsA("Model") then
if not item:FindFirstChild("Req") then
Item:Image("[ğŸ²]"..item.Name, 1, "rbxassetid://"..item.Image.Value, Color3.fromRGB(255,255,255), function()
local function Test()
local args = {
    [1] = item.Name
}

game:GetService("ReplicatedStorage").Events.MobDropAddToInventory:FireServer(unpack(args))
end

Test()
return
end)
else
Item:Image("[ğŸ²]"..item.Name.." Req: "..item.Req.Value, 1, "rbxassetid://"..item.Image.Value, Color3.fromRGB(255,255,255), function()
local function Test()
local args = {
    [1] = item.Name
}

game:GetService("ReplicatedStorage").Events.MobDropAddToInventory:FireServer(unpack(args))
end

Test()
return
end)
end
end
end

