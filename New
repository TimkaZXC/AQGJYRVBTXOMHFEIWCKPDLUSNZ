        if not string.find(eggWN, eggM) then
        print("NOT")
        elseif string.find(eggWN, eggM) then
        Egg:Active("[ü•ö]Open "..egg.Name.." Price: "..eggW.Part.BillboardGui.Frame.Amount.Text, Color3.fromRGN(255,255,0), function()
