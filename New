          [2] = 1
        }

        game:GetService("ReplicatedStorage").Remotes.Eggs.Hatch:InvokeServer(unpack(args))
        game:GetService("ReplicatedStorage").Remotes.Eggs.CancelCooldown:FireServer()
        end)
        end
    end
end

