				Inco2.Size = UDim2.new(0, 4, 0, 25)
				Inco2.LayoutOrder = -1
				Inco2.Parent = Background
				Inco2.BackgroundColor3 = color
				Corner(Inco2, 5)

				Text.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				Text.BackgroundTransparency = 1
				Text.LayoutOrder = 0
				Text.Size = UDim2.new(0, 270, 0, 25)
				Text.Visible = true
				Text.TextColor3 = Color3.fromRGB(255,255,255)
				Text.TextScaled = true
				Text.RichText = true
				Text.Font = Enum.Font.PatrickHand
				Text.TextXAlignment = Enum.TextXAlignment.Center
				Text.Name = "Text"
				Text.Text = tostring(" "..name):upper()
