for _, block in pairs(Blocks:GetChildren()) do
    if not block:IsA("Part") then
        return
    elseif block:IsA("Part") then
        for _, pickaxe in pairs(Pickaxes) do
            for i = 1, 10 do
                Root.CFrame = block.CFrame * CFrame.new(0,4,0)
                local args = {
                    [1] = block,
                    [2] = tonumber(pickaxe)
                }
                    
                EDamage:FireServer(unpack(args))
                wait()
            end
        end
    end    
end
